---
// TituloYDescripcion.astro
interface Props {
  titulo: string
  descripcion: string
}
const { titulo, descripcion } = Astro.props
---

<div class="text-white text-center mt-4">
  <h1 transition:name="Videoclips" class="text-lime-400">{titulo}</h1>
  <p class="text-red">{descripcion}</p>
</div>
